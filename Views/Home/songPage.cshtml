@{
    ViewData["Title"] = "Songs";
}

<div class="wrapper">
    <a href="/logoff">Log Out</a>
    <a href="/">Dashboard</a>
    <h3>Others who added @ViewBag.songInfo.artist - @ViewBag.songInfo.title</h3>
    <table> 
        <tr>
            <th>Name</th>
            <th>Times Added</th>
        </tr>
        @{
            {                
                @foreach(var user in @ViewBag.allUsers)
                {
                    @foreach(var sub in @user.Joins)
                    {
                        <tr>
                            <td><a href="/userPage/@user.Id">@sub.User.first_name @sub.User.last_name</a></td>
                            <td></td>
                        </tr>
                    }
                        
                }
            }
        }
    </table>
</div>

